{
  "name": "w3-bestiary-front",
  "version": "3.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "build:production": "ng build --configuration production",
    "build:preview": "ng build --configuration preview",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run w3-bestiary-front:serve-ssr",
    "serve:ssr": "node dist/w3-bestiary-front/server/main.js",
    "build:ssr": "ng build && ng run w3-bestiary-front:server",
    "prerender": "ng run w3-bestiary-front:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.3",
    "@angular/common": "^17.1.3",
    "@angular/compiler": "^17.1.3",
    "@angular/core": "^17.1.3",
    "@angular/forms": "^17.1.3",
    "@angular/platform-browser": "^17.1.3",
    "@angular/platform-browser-dynamic": "^17.1.3",
    "@angular/platform-server": "^17.1.3",
    "@angular/router": "^17.1.3",
    "@angular/ssr": "^17.2.3",
    "express": "^4.18.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.4",
    "@angular/cli": "^17.1.4",
    "@angular/compiler-cli": "^17.1.3",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "browser-sync": "^3.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.8.8",
    "typescript": "~5.3.3"
  }
}